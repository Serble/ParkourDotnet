<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>MinecraftDotnet.Game.Parkour</PackageId>
        <Title>Parkour</Title>
        <Authors>CoPokBl</Authors>
        <Description>A Parkour gamemode written with MinecraftDotnet.</Description>
        <PackageProjectUrl>https://github.com/Serble/ParkourDotnet</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Serble/ParkourDotnet.git</RepositoryUrl>
        <Version>1.0.0</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MinecraftDotnet.ManagedServer" Version="0.0.1-dev.158" />
    </ItemGroup>

</Project>
